function test65(a, b) {
if (a==65) {
return true; 
} else if (b==65) {
return true;

} else if (a + b == 65) {
return true;
} else {
	return false;
}
}

console.log(test65(60,60))
console.log(test65(65,65))
console.log(test65(5,5))
console.log(test65(30,35))


