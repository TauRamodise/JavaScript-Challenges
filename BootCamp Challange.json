// CHALLANGE 3//

function max_of_three(a, b, c) {
	max_val = 0;
	if (a > b)
	{ 
		max_val = a;
	} else {
		max_val = b;
	} if (c > max_val) {
		max_val = c;
	}
	return max_val
}

console.log(max_of_three(22, 55, 88))
console.log(max_of_three(33, 66, 99))
console.log(max_of_three(44, 77, 111))

// CHALLANGE 4//

function time_convert(val) {
	var hours = Math.floor(val / 60);
	var minutes = val % 60;
	return hours + ":" + minutes;
}

console.log(time_convert(121));
console.log(time_convert(1212));
console.log(time_convert(12121));

